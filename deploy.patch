+  });
+
+  if (transactions.length > 5) {
+    console.log(`   ... 还有 ${transactions.length - 5} 条记录`);
+  }
+
+  // 4. 获取分类信息
+  console.log('\n4. 获取分类信息...');
+  const categoryIds = [...new Set(transactions?.map(t => t.category_id) || [])];
